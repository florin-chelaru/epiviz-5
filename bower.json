{
  "name": "epiviz",
  "version": "5.0.0",
  "authors": [
    "Florin Chelaru <florinc@mit.edu>"
  ],
  "description": "EpiViz is a scientific information visualization tool for genetic and epigenetic data, used to aid in the exploration and understanding of correlations between various genome features.",
  "main": "epiviz.js",
  "moduleType": [
    "globals"
  ],
  "keywords": [
    "visualization",
    "epigenomic"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "vis.js": "git://github.com/florin-chelaru/vis.js.git",
    "vs.ui.plugins.js": "git://github.com/florin-chelaru/vs.ui.plugins.js.git",
    "vs.models.plugins.js": "git://github.com/florin-chelaru/vs.models.plugins.js.git",

    "google-closure-library": "git://github.com/google/closure-library.git#56712dfced32778fc8435a4bea6a2e422e5f03c1",
    "angular": "1.4.4",
    "d3": "3.5.6",
    "jquery": "2.1.4",
    "bootstrap": "3.3.6",
    "angular-bootstrap": "0.14.2",

    "ng-utils": "git://github.com/florin-chelaru/ng-utils.git",
    "ng-bootstrap-controls": "git://github.com/florin-chelaru/ng-bootstrap-controls.git"
  },

  "devDependencies": {
    "closure-compiler": "0.2.7",
    "qunit": "1.19.0"
  }
}
